{
  "headers": [
    {
      "source": "/(.*)\\.(js|mjs)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.squarespace.com https://cotlpet.squarespace.com https://www.cotl.pet https://cotl.pet"
        }
      ]
    }
  ]
}
